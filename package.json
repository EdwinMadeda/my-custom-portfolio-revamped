    "dev": "next dev --turbopack",
    "pre-build": "pnpm run typegen",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typegen": "rm -f sanity.types.ts schema.json src/sanity/extract.json && sanity schema extract && sanity schema extract --path=src/sanity/extract.json && sanity typegen generate",
